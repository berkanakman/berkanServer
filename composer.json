{
    "name": "berkan/server",
    "description": "Berkan - PHP development environment for macOS (Apache & Nginx)",
    "keywords": ["berkan", "apache", "nginx", "php", "development", "macos"],
    "homepage": "https://github.com/berkanakman/berkanServer",
    "license": "MIT",
    "authors": [
        {
            "name": "Berkan Akman"
        }
    ],
    "support": {
        "issues": "https://github.com/berkanakman/berkanServer/issues",
        "source": "https://github.com/berkanakman/berkanServer"
    },
    "require": {
        "php": "^8.1",
        "illuminate/container": "^10.0|^11.0|^12.0",
        "illuminate/collections": "^10.0|^11.0|^12.0",
        "mnapoli/silly": "^1.9",
        "symfony/console": "^6.0|^7.0",
        "symfony/process": "^6.0|^7.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Berkan\\": "cli/Berkan/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Berkan\\Tests\\": "tests/"
        }
    },
    "bin": [
        "berkan"
    ],
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
